<%@ val uri: String = "http://nothing.com" %>
${include("sparql/prefixes.ssp")}

SELECT
  ?license
  ?type
  ?label
  ?number
  ?state

WHERE {
  <${uri}> core:relatedBy ?license.
  ?license a dukecv:MedicalLicensure ;
    vitro:mostSpecificType ?type ;
    rdfs:label ?label ;
    dukecv:licenseState ?state ;
    core:licenseNumber ?number
    }