<%--  -*- sparql -*- --%>
<%@ val uri: String = "http://nothing.com" %>
${include("sparql/prefixes.ssp")}

SELECT ?uri ?type ?label ?linkText ?linkURI
  ("${uri}" AS ?personUri)
WHERE {
  <${uri}> obo:ARG_2000028 ?individualVcard.
  ?individualVcard vcard:hasURL ?uri .
  ?uri vitro:mostSpecificType ?type ;
    rdfs:label ?label ;
    vcard:url ?linkURI .
}
order by ?label
