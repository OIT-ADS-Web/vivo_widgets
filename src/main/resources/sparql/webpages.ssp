<%--  -*- sparql -*- --%>
<%@ val uri: String = "http://nothing.com" %>
${include("sparql/prefixes.ssp")}

SELECT ?uri ?type ?label ?linkText ?linkURI
  ("${uri}" AS ?personUri)
WHERE {
  <${uri}> core:webpage ?uri .
  ?uri vitro:mostSpecificType ?type ;
    rdf:type core:URLLink ;
    core:linkAnchorText ?linkAnchorText ;
    core:linkURI ?linkURI ;
    core:linkAnchorText ?label
}
order by ?label
