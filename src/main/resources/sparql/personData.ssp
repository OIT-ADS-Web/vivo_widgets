<%@ val uri: String = "http://nothing.com" %>
${include("sparql/prefixes.ssp")}

SELECT *
WHERE {
      <${ uri }> core:preferredTitle ?title ;
        rdf:type ?type ;
        rdfs:label ?label ;
        core:overview ?overview ;
        foaf:firstName ?firstName ;
        foaf:lastName ?lastName ;
        core:middleName ?middleName ;
}
